{
  "name": "site",
  "version": "0.1.0",
  "description": "Website of failfast",
  "keywords": [
    "ai",
    "artificial intelligence",
    "javascript",
    "gpt",
    "gpt-4",
    "gpt-35-turbo",
    "web-ui"
  ],
  "license": "UNLICENSED",
  "author": {
    "name": "Tim Pietrusky",
    "url": "https://github.com/TimPietrusky"
  },
  "scripts": {
    "dev": "next dev",
    "prepare": "husky install",
    "spj": "npx sort-package-json",
    "toc": "npx markdown-toc README.md -i",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ncu": "npx npm-check-updates -u --dep='prod,dev'",
    "ncu:minor": "npm run ncu -- -t minor",
    "ncu:patch": "npm run ncu -- -t patch",
    "tsc:noEmit": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/parser": "7.21.4",
    "@emotion/cache": "11.10.7",
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.6",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@monaco-editor/react": "4.5.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.0",
    "@next/mdx": "13.2.4",
    "@types/prettier": "^2.7.2",
    "@use-cookie-consent/react": "0.3.7",
    "axios": "1.3.5",
    "codesandbox": "2.2.3",
    "esdeka": "0.1.18",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "gpt3-tokenizer": "1.1.5",
    "jotai": "2.0.4",
    "monaco-editor": "0.37.1",
    "nanoid": "4.0.2",
    "next": "13.2.4",
    "next-auth": "4.22.1",
    "openai": "^3.2.1",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remark-gfm": "3.0.1",
    "swr": "^2.2.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/node": "18.15.11",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "husky": "^8.0.3",
    "typescript": "5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,css,scss,md}": [
      "npx prettier --write"
    ],
    "package.json": [
      "npx sort-package-json",
      "npx prettier --write"
    ],
    "*.md": [
      "npx prettier --write"
    ],
    "README.md": [
      "npx markdown-toc -i"
    ]
  }
}
